{
  "name": "ProtractorTutorial",
  "dependencies": {
    "protractor": "^5.3.0",
    "jasmine-data-provider": "^2.2.0",
    "protractor-jasmine2-html-reporter": "^0.0.7"
  },
  "scripts": {
    "webdriver-update": "./node_modules/.bin/webdriver-manager update",
    "webdriver-start": "./node_modules/.bin/webdriver-manager start",
    "e2e": "./node_modules/.bin/protractor configuration.js",
    "smoke": "./node_modules/.bin/protractor ./Configs/configuration.js --suite Smokesuite",
    "regression": "./node_modules/.bin/protractor ./Configs/configuration.js --suite Regressionsuite",
    "start": "npm run webdriver-update && npm run webdriver-start",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
