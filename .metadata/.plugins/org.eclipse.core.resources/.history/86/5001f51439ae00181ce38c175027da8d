//**********************************************
var c = "one";
if(c=="one")
	{
		console.log("statement is true");
	}
else
	{
		console.log("statement is false");
	}

//**********************************************
var d = "three";

if(d=="one")
	{
		console.log("statement is one");
	}
else if(d=="two")
	{
		console.log("statement is two");
	}
else if(d=="three")
	{
		console.log("statement is three");
	}
else
	{
		console.log("statement is not matched");
	}

//**********************************************
for(var i = 0; i <= 100; i=i+5)
	{
		console.log(i);
	}

//**********************************************
var j=1;
while(j<=5)
	{
		console.log(j);
		j++;
	}

//**********************************************
var k = 10;
do
	{
		console.log(k);
	}while(k<6)
	
