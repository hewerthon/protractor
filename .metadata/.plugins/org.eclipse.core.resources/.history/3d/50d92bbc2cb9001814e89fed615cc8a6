var obj = require("./Jsobject.js")

/**
First parameter - test suite name
Second parameter - function (function will have all tests (it blocks)
*/
describe('Protractor baby steps',function()
		{
			it('Open Angular js website',function(){
				browser.waitForAngularEnabled(false);
				browser.get("http://google.com");
				browser.get('http://juliemr.github.io/protractor-demo/');
				// write yout raw protractor code
			    obj.firstinput.sendKeys(1);
			    obj.secondinput.sendKeys(2);

			    obj.goButton.click();

			    expect(obj.result.getText()).toBe('3'); // This is wrong!
			    
			    obj.result.getText().then(function(text){
			    	console.log(text);
			    })
			    //expect(element(by.binding('latest')).getText()).toBe('3'); // This is wrong!
			    //element(by.css("h2[class='ng-binding']")).getText().then(function(text){
			    //console.log(text);	
			    //})
			    browser.sleep(3000);
			    
				
			})

			it('close browswer', function() {
			// code to close browser
				
			})
		})
		
