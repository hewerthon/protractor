{
  "name": "ProtractorTutorial",
  "dependencies":{
  					"protractor": "^5.3.0" 
 	},
  "scripts": {
    "webdriver-start": "./node_modules/.bin/webdriver-manager update",
    "we": "./node_modules/.bin/webdriver-manager start",
    "protractor": "./node_modules/.bin/protractor conf.js",
    "start": "npm run webdriver-start && npm run we && npm run protractor",
    "test": "echo \"Error: no test specified\" && exit 1"
  }

