			    })
			   
			    expect(element(by.binding('latest')).getText()).toBe('3'); // This is wrong!
			    element(by.css("h2[class='ng-binding']")).getText().then(function(text){
			    console.log(text);	
			    })
			    */
			    browser.sleep(3000);
			})
			
			afterEach(function(){
				console.log("Test is completed");
			});

